{
  "name": "LinkrDev",
  "version": "1.0.0",
  "description": "LinkrDev 全栈项目",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd LinkrWeb && npm run dev",
    "dev:backend": "cd LinkrMix && mvn spring-boot:run",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd LinkrWeb && npm run build",
    "build:backend": "cd LinkrMix && mvn clean package -DskipTests",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd LinkrWeb && npm run preview",
    "start:backend": "cd LinkrMix && java -jar admin-service/target/admin-service-1.0-SNAPSHOT.jar",
    "install:all": "npm install && cd LinkrWeb && npm install",
    "clean": "cd LinkrWeb && rm -rf dist && cd LinkrMix && mvn clean",
    "deploy:local": "npm run build && npm run start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}